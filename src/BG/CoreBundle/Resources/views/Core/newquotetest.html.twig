{% extends "BGCoreBundle::layout.html.twig" %}

{% block body %}

  <div class="container">
    <div class="row">
      <div class="col-sm-8 col-sm-offset-2">
        <!--Wizard container-->
        <div class="wizard-container">
          <div class="card wizard-card" data-color="blue" id="wizard" style="padding:2vh;">

            <div class="wizard-header">
              <h3 class="wizard-title">AJOUTER UN DEVIS</h3>
            </div>

            <button class="btn btn-fill btn-info" type="button" id="add_service">Ajouter un service</button>

            {{ form(form) }}

          </div>
        </div>
      </div>
    </div>
  </div>

  {{ parent() }}

{% endblock %}
